#: src/mpop.c:2639
#, c-format
msgid ""
"Usage:\n"
"\n"
msgstr ""
"Користування:\n"
"\n"

#: src/mpop.c:2640
#, c-format
msgid ""
"Mail retrieval mode (default):\n"
"  %s [option...] [--] account...\n"
"  %s --host=host [option...]\n"
"  Read mails from one ore more POP3 accounts and deliver them.\n"
msgstr ""
"Режим отримання пошти (типовий):\n"
"  %s [параметр...] [--] обліковий_запис...\n"
"  %s --host=вузол [параметр...]\n"
"  Прочитати повідомлення з одного або декількох записів POP3 і розіслати надіслані повідомлення.\n"

#: src/mpop.c:2645
#, c-format
msgid ""
"Server information mode:\n"
"  %s [option...] --serverinfo account...\n"
"  %s --host=host [option...] --serverinfo\n"
"  Print information about one or more POP3 servers.\n"
"\n"
msgstr ""
"Режим даних щодо сервера:\n"
"  %s [параметр...] --serverinfo обліковий_запис...\n"
"  %s --host=host [параметр...] --serverinfo\n"
"  Вивести дані щодо одного або декількох серверів POP3.\n"
"\n"

#: src/mpop.c:2649
#, c-format
msgid "General options:\n"
msgstr "Загальні параметри:\n"

#: src/mpop.c:2650
#, c-format
msgid "  --version                    print version\n"
msgstr "  --version                    вивести дані щодо версії\n"

#: src/mpop.c:2651
#, c-format
msgid "  --help                       print help\n"
msgstr "  --help                       вивести додаткове повідомлення\n"

#: src/mpop.c:2652
#, c-format
msgid "  -P, --pretend                print configuration info and exit\n"
msgstr "  -P, --pretend                вивести дані щодо налаштування і завершити роботу\n"

#: src/mpop.c:2653
#, c-format
msgid "  -d, --debug                  print debugging information\n"
msgstr "  -d, --debug                  вивести діагностичну інформацію\n"

#: src/mpop.c:2654
#, c-format
msgid "Changing the mode of operation:\n"
msgstr "Зміна режиму роботи:\n"

#: src/mpop.c:2655
#, c-format
msgid "  --configure=mailaddress      generate and print configuration for address\n"
msgstr "  --configure=поштова_адреса   створити і вивести налаштування для вказаної адреси\n"

#: src/mpop.c:2656
#, c-format
msgid "  -S, --serverinfo             print information about the server\n"
msgstr "  -S, --serverinfo             вивести відомості щодо сервера\n"

#: src/mpop.c:2657
#, c-format
msgid "Configuration options:\n"
msgstr "Параметри налаштовування:\n"

#: src/mpop.c:2658
#, c-format
msgid "  -C, --file=filename          set configuration file\n"
msgstr "  -C, --file=назва_файла       вказати файл налаштувань\n"

#: src/mpop.c:2659
#, c-format
msgid ""
"  --host=hostname              set the server, use only command-line settings;\n"
"                               do not use any configuration file data\n"
msgstr ""
"  --host=назва_вузла           вказати сервер, використовувати лише параметри командного рядка;\n"
"                               не використовувати дані з файла налаштувань\n"

#: src/mpop.c:2661
#, c-format
msgid "  --port=number                set port number\n"
msgstr "  --port=число                 вказати номер порту\n"

#: src/mpop.c:2662
#, c-format
msgid "  --proxy-host=[IP|hostname]   set/unset proxy\n"
msgstr "  --proxy-host=[IP|вузол]     визначити/скасувати визначення проксі\n"

#: src/mpop.c:2663
#, c-format
msgid "  --proxy-port=[number]        set/unset proxy port\n"
msgstr "  --proxy-port=[число]         вказати/скасувати визначення порту проксі\n"

#: src/mpop.c:2664
#, c-format
msgid "  --source-ip=[IP]             set/unset source ip address to bind the socket to\n"
msgstr "  --source-ip=[IP]             вказати/скасувати визначення ip-адреси для прив'язування сокета\n"

#: src/mpop.c:2665
#, c-format
msgid "  --timeout=(off|seconds)      set/unset network timeout in seconds\n"
msgstr "  --timeout=(off|секунди)      вказати/скасувати визначення часу очікування у секундах\n"

#: src/mpop.c:2666
#, c-format
msgid "  --pipelining=(auto|on|off)   enable/disable pipelining\n"
msgstr "  --pipelining=(auto|on|off)   увімкнути/вимкнути конвеєрну обробку\n"

#: src/mpop.c:2667
#, c-format
msgid "  --received-header[=(on|off)] enable/disable Received-header\n"
msgstr "  --received-header[=(on|off)] увімкнути/вимкнути заголовок Received\n"

#: src/mpop.c:2668
#, c-format
msgid "  --auth[=(on|method)]         choose the authentication method\n"
msgstr "  --auth[=(on|спосіб)]         вибрати спосіб розпізнавання\n"

#: src/mpop.c:2669
#, c-format
msgid "  --user=[username]            set/unset user name for authentication\n"
msgstr "  --user=[користувач]          вказати/скасувати визначення користувача для розпізнавання\n"

#: src/mpop.c:2670
#, c-format
msgid "  --passwordeval=[eval]        evaluate password for authentication\n"
msgstr "  --passwordeval=[eval]        перевірити пароль для розпізнавання\n"

#: src/mpop.c:2671
#, c-format
msgid "  --tls[=(on|off)]             enable/disable TLS encryption\n"
msgstr "  --tls[=(on|off)]             увімкнути/вимкнути шифрування TLS\n"

#: src/mpop.c:2672
#, c-format
msgid "  --tls-starttls[=(on|off)]    enable/disable STARTTLS for TLS\n"
msgstr "  --tls-starttls[=(on|off)]    увімкнути/вимкнути STARTTLS для TLS\n"

#: src/mpop.c:2673
#, c-format
msgid "  --tls-trust-file=[file]      set/unset trust file for TLS\n"
msgstr "  --tls-trust-file=[файл]      вказати/скасувати визначення файла довіри для TLS\n"

#: src/mpop.c:2674
#, c-format
msgid "  --tls-crl-file=[file]        set/unset revocation file for TLS\n"
msgstr "  --tls-crl-file=[файл]        вказати/скасувати визначення файла відкликання для TLS\n"

#: src/mpop.c:2675
