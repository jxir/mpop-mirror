msgid ""
"Usage:\n"
"\n"
msgstr ""
"Користування:\n"
"\n"

#: src/mpop.c:2444
#, c-format
msgid ""
"Mail retrieval mode (default):\n"
"  %s [option...] [--] account...\n"
"  %s --host=host [option...]\n"
"  Read mails from one ore more POP3 accounts and deliver them.\n"
msgstr ""
"Режим отримання пошти (типовий):\n"
"  %s [параметр...] [--] обліковий_запис...\n"
"  %s --host=вузол [параметр...]\n"
"  Прочитати повідомлення з одного або декількох записів POP3 і розіслати надіслані повідомлення.\n"

#: src/mpop.c:2449
#, c-format
msgid ""
"Server information mode:\n"
"  %s [option...] --serverinfo account...\n"
"  %s --host=host [option...] --serverinfo\n"
"  Print information about one or more POP3 servers.\n"
"\n"
msgstr ""
"Режим даних щодо сервера:\n"
"  %s [параметр...] --serverinfo обліковий_запис...\n"
"  %s --host=host [параметр...] --serverinfo\n"
"  Вивести дані щодо одного або декількох серверів POP3.\n"
"\n"

#: src/mpop.c:2453
#, c-format
msgid "General options:\n"
msgstr "Загальні параметри:\n"

#: src/mpop.c:2454
#, c-format
msgid "  --version                    print version\n"
msgstr "  --version                    вивести дані щодо версії\n"

#: src/mpop.c:2455
#, c-format
msgid "  --help                       print help\n"
msgstr "  --help                       вивести додаткове повідомлення\n"

#: src/mpop.c:2456
#, c-format
msgid "  -P, --pretend                print configuration info and exit\n"
msgstr "  -P, --pretend                вивести дані щодо налаштування і завершити роботу\n"

#: src/mpop.c:2457
#, c-format
msgid "  -d, --debug                  print debugging information\n"
msgstr "  -d, --debug                  вивести діагностичну інформацію\n"

#: src/mpop.c:2458
#, c-format
msgid "Changing the mode of operation:\n"
msgstr "Зміна режиму роботи:\n"

#: src/mpop.c:2459
#, c-format
msgid "  -S, --serverinfo             print information about the server\n"
msgstr "  -S, --serverinfo             вивести відомості щодо сервера\n"

#: src/mpop.c:2460
#, c-format
msgid "Configuration options:\n"
msgstr "Параметри налаштовування:\n"

#: src/mpop.c:2461
#, c-format
msgid "  -C, --file=filename          set configuration file\n"
msgstr "  -C, --file=назва_файла       вказати файл налаштувань\n"

#: src/mpop.c:2462
#, c-format
msgid ""
"  --host=hostname              set the server, use only command-line settings;\n"
"                               do not use any configuration file data\n"
msgstr ""
"  --host=назва_вузла           вказати сервер, використовувати лише параметри командного рядка;\n"
"                               не використовувати дані з файла налаштувань\n"

#: src/mpop.c:2464
#, c-format
msgid "  --port=number                set port number\n"
msgstr "  --port=число                 вказати номер порту\n"

#: src/mpop.c:2465
#, c-format
msgid "  --proxy-host=[IP|hostname]   set/unset proxy\n"
msgstr "  --proxy-host=[IP|вузол]     визначити/скасувати визначення проксі\n"

#: src/mpop.c:2466
#, c-format
msgid "  --proxy-port=[number]        set/unset proxy port\n"
msgstr "  --proxy-port=[число]         вказати/скасувати визначення порту проксі\n"

#: src/mpop.c:2467
#, c-format
msgid "  --source-ip=[IP]             set/unset source ip address to bind the socket to\n"
msgstr "  --source-ip=[IP]             вказати/скасувати визначення ip-адреси для прив'язування сокета\n"

#: src/mpop.c:2468
#, c-format
msgid "  --timeout=(off|seconds)      set/unset network timeout in seconds\n"
msgstr "  --timeout=(off|секунди)      вказати/скасувати визначення часу очікування у секундах\n"

#: src/mpop.c:2469
#, c-format
msgid "  --pipelining=(auto|on|off)   enable/disable pipelining\n"
msgstr "  --pipelining=(auto|on|off)   увімкнути/вимкнути конвеєрну обробку\n"

#: src/mpop.c:2470
#, c-format
msgid "  --received-header[=(on|off)] enable/disable Received-header\n"
msgstr "  --received-header[=(on|off)] увімкнути/вимкнути заголовок Received\n"

#: src/mpop.c:2471
#, c-format
msgid "  --auth[=(on|method)]         choose the authentication method\n"
msgstr "  --auth[=(on|спосіб)]         вибрати спосіб розпізнавання\n"

#: src/mpop.c:2472
#, c-format
msgid "  --user=[username]            set/unset user name for authentication\n"
msgstr "  --user=[користувач]          вказати/скасувати визначення користувача для розпізнавання\n"

#: src/mpop.c:2473
#, c-format
msgid "  --passwordeval=[eval]        evaluate password for authentication\n"
msgstr "  --passwordeval=[eval]        перевірити пароль для розпізнавання\n"

#: src/mpop.c:2474
#, c-format
msgid "  --tls[=(on|off)]             enable/disable TLS encryption\n"
msgstr "  --tls[=(on|off)]             увімкнути/вимкнути шифрування TLS\n"

#: src/mpop.c:2475
#, c-format
msgid "  --tls-starttls[=(on|off)]    enable/disable STARTTLS for TLS\n"
msgstr "  --tls-starttls[=(on|off)]    увімкнути/вимкнути STARTTLS для TLS\n"

#: src/mpop.c:2476
#, c-format
msgid "  --tls-trust-file=[file]      set/unset trust file for TLS\n"
msgstr "  --tls-trust-file=[файл]      вказати/скасувати визначення файла довіри для TLS\n"

#: src/mpop.c:2477
#, c-format
msgid "  --tls-crl-file=[file]        set/unset revocation file for TLS\n"
msgstr "  --tls-crl-file=[файл]        вказати/скасувати визначення файла відкликання для TLS\n"

#: src/mpop.c:2478
#, c-format
msgid "  --tls-fingerprint=[f]        set/unset trusted certificate fingerprint for TLS\n"
msgstr "  --tls-fingerprint=[f]        вказати/скасувати визначення відбитка надійного сертифіката для TLS\n"

#: src/mpop.c:2479
#, c-format
msgid "  --tls-key-file=[file]        set/unset private key file for TLS\n"
msgstr "  --tls-key-file=[файл]        вказати/скасувати визначення файла закритого ключа для TLS\n"

#: src/mpop.c:2480
#, c-format
msgid "  --tls-cert-file=[file]       set/unset private cert file for TLS\n"
msgstr "  --tls-cert-file=[файл]        вказати/скасувати визначення файла закритого сертифіката для TLS\n"

#: src/mpop.c:2481
#, c-format
msgid "  --tls-certcheck[=(on|off)]   enable/disable server certificate checks for TLS\n"
msgstr "  --tls-certcheck[=(on|off)]   увімкнути/вимкнути перевірки сертифіката для TLS\n"

#: src/mpop.c:2482
#, c-format
msgid "  --tls-min-dh-prime-bits=[b]  set/unset minimum bit size of DH prime\n"
msgstr "  --tls-min-dh-prime-bits=[b]  встановити/скасувати визначення мінімального розміру у бітах для основи DH\n"

#: src/mpop.c:2483
#, c-format
msgid "  --tls-priorities=[prios]     set/unset TLS priorities.\n"
msgstr "  --tls-priorities=[пріор]     вказати/скасувати визначення пріоритетностей TLS.\n"

#: src/mpop.c:2484
#, c-format
msgid "Options specific to mail retrieval mode:\n"
msgstr "Параметри, які є специфічними для режиму отримання пошти:\n"

#: src/mpop.c:2485
#, c-format
msgid "  -q, --quiet                  do not display status or progress information\n"
msgstr "  -q, --quiet                  не виводити даних щодо стану та поступу\n"

#: src/mpop.c:2486
#, c-format
msgid "  -Q, --half-quiet             display status but not progress information\n"
msgstr "  -Q, --half-quiet             вивести дані щодо стану, але не виводити даних щодо поступу\n"

#: src/mpop.c:2487
#, c-format
msgid "  -a, --all-accounts           query all accounts in the configuration file\n"
msgstr "  -a, --all-accounts           опитати усі облікові записи із файла налаштувань\n"

#: src/mpop.c:2488
#, c-format
msgid "  -A, --auth-only              authenticate only; do not retrieve mail\n"
msgstr "  -A, --auth-only              лише пройти розпізнавання; не отримувати пошти\n"

#: src/mpop.c:2489
#, c-format
msgid "  -s, --status-only            print account status only; do not retrieve mail\n"
msgstr "  -s, --status-only            вивести лише стан облікового запису; не отримувати пошти\n"

#: src/mpop.c:2490
#, c-format
msgid "  -n, --only-new[=(on|off)]    process only new messages\n"
msgstr "  -n, --only-new[=(on|off)]    обробити лише нові повідомлення\n"

#: src/mpop.c:2491
#, c-format
msgid "  -k, --keep[=(on|off)]        do not delete mails from servers\n"
msgstr "  -k, --keep[=(on|off)]        не вилучати повідомлення з серверів\n"

#: src/mpop.c:2492
#, c-format
msgid "  --killsize=(off|number)      set/unset kill size\n"
msgstr "  --killsize=(off|число)       вказати/скасувати визначення розміру вилучення\n"

#: src/mpop.c:2493
#, c-format
msgid "  --skipsize=(off|number)      set/unset skip size\n"
msgstr "  --skipsize=(off|число)       вказати/скасувати визначення розміру пропускання\n"

#: src/mpop.c:2494
#, c-format
msgid "  --filter=[program]           set/unset header filter\n"
msgstr "  --filter=[програма]          вказати/скасувати визначення фільтра заголовків\n"

#: src/mpop.c:2495
#, c-format
msgid "  --delivery=method,arg        set the mail delivery method\n"
msgstr "  --delivery=спосіб,аргумент   встановити спосіб доставлення пошти\n"

#: src/mpop.c:2496
#, c-format
msgid "  --uidls-file=filename        set file to store UIDLs\n"
msgstr "  --uidls-file=файл           вказати файл для зберігання UIDL\n"

